{
  "name": "nearestdepartures-svelte",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_APP_VERSION=$npm_package_version vite",
    "prebuild": "npx vitest run",
    "build": "cross-env VITE_APP_VERSION=$npm_package_version vite build",
    "preview": "cross-env VITE_APP_VERSION=$npm_package_version vite preview",
    "predeploy:test": "npm run build",
    "deploy:test": "gh-pages -d dist",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint ./src/**/*.{ts,svelte}"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/ramda": "^0.28.15",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.1",
    "lucide-svelte": "^0.91.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "ramda": "^0.28.0"
  }
}
