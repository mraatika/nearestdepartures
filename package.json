{
  "name": "nearestdepartures",
  "version": "0.1.0",
  "description": "An app for finding nearest public transport departures in Helsinki region using the digitransit api",
  "repository": {
    "type": "git",
    "url": "https://github.com/mraatika/nearestdepartures"
  },
  "keywords": [
    "inferno",
    "infernojs",
    "digitransit",
    "HSL",
    "publictransport"
  ],
  "author": "Miitri Raatikainen (http://github.com/mraatika)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mraatika/nearestdepartures/issues"
  },
  "homepage": "https://mraatika.github.io/nearestdepartures",
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.0",
    "babel-plugin-inferno": "^2.0.0",
    "babel-preset-inferno-app": "^2.0.0",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "cheerio": "^0.22.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "5.1.0",
    "css-loader": "0.26.4",
    "detect-port": "1.1.0",
    "dotenv": "4.0.0",
    "eslint": "^3.18.0",
    "eslint-config-inferno-app": "^1.0.0",
    "eslint-loader": "1.6.3",
    "eslint-plugin-flowtype": "2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-inferno": "6.10.2",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "fs-extra": "2.0.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "inferno": "^1.3.1",
    "inferno-component": "^1.3.1",
    "inferno-dev-utils": "^1.0.0",
    "inferno-server": "~1.4.2",
    "jest": "19.0.2",
    "mock-geolocation": "^1.0.11",
    "object-assign": "4.1.1",
    "offline-plugin": "^4.6.2",
    "postcss-loader": "1.3.3",
    "promise": "7.1.1",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "babel-plugin-inferno": "^2.1.0",
    "inferno": "~1.4.2",
    "inferno-component": "~1.4.2",
    "lodash": "^4.17.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^inferno-native$": "inferno-native-web"
    }
  },
  "babel": {
    "presets": [
      "inferno-app"
    ]
  },
  "eslintConfig": {
    "rules": {
      "import/no-unresolved": "error",
      "import/named": "error",
      "import/no-extraneous-dependencies": "error"
    },
    "extends": "inferno-app"
  }
}
